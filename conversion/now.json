{
  "version": 2,
  "regions": [
    "bru1"
  ],
  "builds": [
    {
      "src": "public/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/v1/bite.js",
      "dest": "/public/bite.v1.js",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "/.*",
      "status": 404
    }
  ]
}
