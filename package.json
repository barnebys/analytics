  },
  "dependencies": {
    "@barnebys/fingerprint": "^1.0.0",
    "@google-cloud/bigquery": "^6.0.0",
    "@vercel/ncc": "^0.34.0",
    "emptygif": "^0.1.2",
    "encodeurl": "^1.0.2",
    "faunadb": "^2.11.1",
    "ip-anonymize": "^0.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "md5": "^2.3.0",
    "micro": "^9.3.4",
    "micro-cookie-session": "^2.0.0-beta.1",
    "microrouter": "^3.1.3",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.6",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "micro-dev": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tinyify": "^2.5.2"
  },
  "babel": {
    "plugins": [
      "transform-inline-environment-variables"
